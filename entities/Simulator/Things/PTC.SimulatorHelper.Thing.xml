                                                var friendlyName =  Things[row.name].FriendlyName; 
                                                var rowObject = new Object(); 
                                                rowObject.Name = row.name; 
                                                rowObject.FriendlyName = friendlyName; 
                                                rowObject.ID = "NOT SET"; 
                                                result.AddRow(rowObject); 
                                            } 
                                             
                                        } catch (err) { 
