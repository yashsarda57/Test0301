                                    <![CDATA[
                                    logger.debug("Entering " + me.name +": RandomGenerator" ); 
                                      
                                    var message =""; 
                                    try { 
                                         
                                      
                                        //Set  value to be a random number between 0 and 360 or the incoming random initialization value 
                                        me.yaw = Math.floor((Math.random() * randomYaw) + 1); 
                                        me.pitch =Math.floor((Math.random() * randomPitch) + 1); 
                                        me.roll = Math.floor((Math.random() * randomRoll) + 1); 
                                      
                                        //Set measurement value to be a random number between 0 and 100 
                                        me.hum = Math.random() * 100; 
                                        me.temp = Math.random() * 100; 
                                      
                                        message = message + "Yaw=" + me.yaw + " "; 
                                        message = message + "Pitch=" + me.pitch+ " "; 
                                        message = message + "Roll=" + me.roll+ " "; 
                                        message = message + "Hum=" + me.hum+ " "; 
                                        message = message + "Temp=" +me.temp+ " "; 
                                      
                                    } catch(ex) { 
                                        message = "An error occured in >>>>" + me.name +": RandomGenerator Exception " + ex; 
                                    } 
                                    logger.debug(me.name + "  RandomGenerator values= " + message ); 
                                      
                                    result = message;
                                    ]]>
                                    </code>
                                </Row>
                            </Rows>
                        </ConfigurationTable>
